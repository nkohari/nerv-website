{
  "name": "nerv-website",
  "version": "0.1.0",
  "description": "nerv.io website",
  "engines": {
    "node": "^6.9.0",
    "npm": "5"
  },
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production node -r ts-node/register ./node_modules/.bin/webpack --progress --optimize-minimize --config ./build/webpack.config.production.ts",
    "dev": "NODE_ENV=development node -r ts-node/register ./node_modules/.bin/webpack-dev-server --hot --config ./build/webpack.config.development.ts",
    "clean": "rimraf dist",
    "deploy": "npm run build && s3-deploy './dist/**' --cwd './dist/' --gzip --region us-east-1 --bucket nerv.io"
  },
  "author": "Nate Kohari",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "svg-react-loader": "^0.4.4"
  },
  "devDependencies": {
    "@types/node": "^6.0.79",
    "@types/react": "^15.0.33",
    "@types/react-dom": "^15.5.1",
    "@types/webpack": "^3.0.1",
    "@types/webpack-env": "^1.13.0",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "nerv-tslint-rules": "github:nkohari/nerv-tslint-rules",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.3.1",
    "s3-deploy": "^0.7.5",
    "style-loader": "0.17.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-loader": "^2.2.1",
    "ts-node": "^3.1.0",
    "tslint": "^5.4.3",
    "tslint-microsoft-contrib": "^5.0.0",
    "typescript": "2.3.3",
    "url-loader": "^0.5.7",
    "webpack": "2.6.1",
    "webpack-dev-server": "^2.5.0"
  }
}
